<odoo>
    <data>
		<record id="proposal_email_template" model="mail.template">
	    <field name="name">PROPOSAL EMAIL TEMPLATE</field>
	    <field name="model_id" ref="proposal_week_2.model_proposals_proposals"/>
	    <field name="auto_delete" eval="True"/>
	    <field name="email_from">${object.sales_man_id.partner_id.email}</field>
	    <field name="email_to">${object.customer_id.email}</field>  
	    <field name="subject">Proposal For ${object.proposal_name}</field>
	    <field name="body_html">
			<p>Dear ${object.customer_name.name},<br/><br/></p>
			Please find your proposal in below link<br/>
		    Regards,<br/>
		    ${object.sales_man_id.name}</field>
		</record>
	</data>
</odoo>