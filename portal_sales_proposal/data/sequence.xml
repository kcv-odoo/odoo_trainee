<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Sequences for sales proposal-->
        <record id="sequence_portal_sales_proposal" model="ir.sequence">
            <field name="name">Portal Sales Proposal</field>
            <field name="code">portal.sales.proposal</field>
            <field name="prefix">PRO/</field>
