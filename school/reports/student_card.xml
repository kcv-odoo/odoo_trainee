<odoo>

	<template id="report_student">
		<t t-call="web.external_layout">
		<t t-call="web.html_container">
			<p><h2><strong> Student Card</strong></h2></p>
			<br/><br/><br/>
			<t t-foreach="docs" t-as="doc">
				<strong>Name :</strong> <t t-esc="doc.name"/><br/><br/>
				<strong>Age :</strong> <t t-esc="doc.age"/><br/><br/>
				<strong>Gender :</strong> <t t-esc="doc.gender"/><hr/>
			</t>
		</t>
		</t>
	</template>

</odoo>