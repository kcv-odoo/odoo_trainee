<odoo>
	<data>
		<record id="email_template" model="mail.template">
			<field name="name"> Send By Email</field>
			<field name="model_id" ref="proposal.model_proposal_order"/>
			<field name="email_from">${object.user_id.email |safe}</field>
			<field name="email_to">${object.customer_name.email}</field>
			<field name="subject"> Proposal ${object.product_name}</field>
			<field name="body_html" type="html">
				<div>
					<p>
						Dear ${object.customer_name.name}<br></br>
						This is your proposal ${object.product_name}<br> </br>
						Please feel free to contact for further queries or clarifications
					</p>
				</div>
			</field>
		</record>
	</data>	
</odoo>